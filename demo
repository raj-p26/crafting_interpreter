print "one";
print true;

let a = 'global a';
let b = 'global b';
let c = 'global c';
{
    let a = 'outer a';
    let b = 'outer b';
    {
        let a = 'inner a';
        print a;
        print b;
        print c;
    }
    print a;
    print b;
    print c;
}
print a;
print b;
print c;

